   if(n==0)return false;
        int i=0;
        
        while(n!=1)
        {
            if(n%2==0)
            {
                // store=n/2;
                n=n/2;
            }
            else if(n%3==0)
            {
                // store=n/3;
                n=n/3;
            }
            else if(n%5==0)
            {
                // store=n/5;
                n=n/5;
            }
            else return false;
        }
        return true;
